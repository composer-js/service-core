        "@typescript-eslint/eslint-plugin": "^8.7.0",
        "@typescript-eslint/parser": "^8.7.0",
        "connect-redis": "^7.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "coveralls": "^3.1.1",
        "eslint": "^9.11.1",
