{
    "name": "@composer-js/service-core",
    "version": "8.0.0-rc.16",
    "description": "Provides all core functionality for ComposerJS based backend services.",
    "repository": "https://gitlab.com/AcceleratXR/composerjs/composer-service-core.git",
    "license": "MIT",
    "author": "AcceleratXR, Inc. <info@acceleratxr.com>",
    "scripts": {
        "build": "yarn lint && rimraf dist && tsc",
        "deploy-docs": "ts-node tools/gh-pages-publish",
        "docs": "typedoc --plugin typedoc-plugin-markdown --out docs/reference src",
        "lint": "eslint ./src ./test",
        "local-audit": "yarn install && yarn npm audit -A -R \"$@\"",
        "checkup": "yarn dlx @yarnpkg/doctor \"$@\"",
        "dependency-check": "yarn dlx depcheck \"$@\"",
        "report-coverage": "cat ./coverage/lcov.info | coveralls",
        "test": "jest -i --forceExit",
        "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
        "test:watch": "node_modules/.bin/jest --detectOpenHandles --watch",
        "postversion": "git push && git push --tags"
    },
    "dependencies": {
        "axios": "^1.7.7",
        "dayjs": "^1.11.13",
        "deepmerge": "^4.3.1",
        "node-schedule": "^2.1.1",
        "response-time": "^2.3.2",
        "swagger-ui-express": "^5.0.1",
        "winston-transport": "^4.7.1"
    },
    "peerDependencies": {
        "@composer-js/core": "6.x",
        "connect-redis": "7.x",
        "cors": "2.x",
        "express": "4.x",
        "express-session": "1.x",
        "ioredis": "5.x",
        "lodash": "4.x",
        "mongodb": "6.x",
        "openapi3-ts": "4.x",
        "passport": "0.7.x",
        "passport-strategy": "1.x",
        "prom-client": "15.x",
        "reflect-metadata": "0.2.x",
        "winston": "3.x",
        "ws": "8.x"
    },
    "devDependencies": {
        "@composer-js/core": "^6.0.0-rc.19",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/ioredis-mock": "^8.2.5",
        "@types/jest": "^29.5.13",
        "@types/node": "^20.16.6",
        "@types/node-schedule": "^2.1.7",
        "@types/passport": "^1.0.16",
        "@types/passport-strategy": "^0.2.38",
        "@types/response-time": "^2.3.8",
