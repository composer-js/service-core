{
    "name": "@composer-js/service-core",
    "version": "4.1.0",
    "description": "Provides all core functionality for ComposerJS based backend services.",
    "repository": "https://gitlab.com/AcceleratXR/composerjs/composer-service-core.git",
    "license": "MIT",
    "author": "AcceleratXR, Inc. <info@acceleratxr.com>",
    "scripts": {
        "build": "rimraf dist && tsc",
        "deploy-docs": "ts-node tools/gh-pages-publish",
        "docs": "typedoc --plugin typedoc-plugin-markdown --out docs/reference src",
        "lint": "eslint -c .eslintrc.js --ext .ts ./src ./test",
        "local-audit": "yarn install && yarn npm audit -A -R \"$@\"",
        "checkup": "yarn dlx @yarnpkg/doctor \"$@\"",
        "dependency-check": "yarn dlx depcheck \"$@\"",
        "report-coverage": "cat ./coverage/lcov.info | coveralls",
        "test": "jest -i --forceExit",
        "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
        "test:watch": "node_modules/.bin/jest --detectOpenHandles --watch",
        "postversion": "git push && git push --tags"
    },
    "dependencies": {
        "@composer-js/core": "^4.0.0",
        "dayjs": "^1.11.10",
        "node-schedule": "^2.1.1",
        "response-time": "^2.3.2",
        "swagger-ui-express": "^5.0.0",
        "winston-transport": "^4.7.0"
    },
    "peerDependencies": {
        "cors": "2.x",
        "express": "4.x",
        "express-session": "1.x",
        "ioredis": "5.x",
        "mongodb": "4.x",
        "openapi3-ts": "4.x",
        "passport": "0.7.x",
        "passport-strategy": "1.x",
        "prom-client": "15.x",
        "reflect-metadata": "0.2.x",
        "winston": "3.x",
        "ws": "8.x"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/ioredis-mock": "^8.2.5",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.7",
        "@types/node-schedule": "^2.1.7",
        "@types/passport": "^1.0.16",
        "@types/passport-strategy": "^0.2.38",
        "@types/response-time": "^2.3.8",
        "@types/rimraf": "^4.0.5",
        "@types/sqlite3": "^3.1.11",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^9.0.8",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "coveralls": "^3.1.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsdoc": "^48.2.3",
        "express": "^4.19.2",
        "express-session": "^1.18.0",
        "ioredis": "^5.4.0",
        "ioredis-mock": "^8.9.0",
        "jest": "^29.7.0",
        "lodash": "^4.17.21",
        "mongodb": "^4.17.2",
        "mongodb-memory-server": "^9.1.8",
        "nconf": "^0.12.1",
        "openapi3-ts": "^4.3.1",
        "passport": "^0.7.0",
        "passport-strategy": "^1.0.0",
        "pg": "^8.11.5",
        "prom-client": "^15.1.2",
        "reflect-metadata": "^0.2.2",
        "rimraf": "^5.0.5",
        "sqlite3": "^5.1.7",
        "supertest": "^6.3.4",
        "superwstest": "^2.0.3",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typedoc": "^0.25.13",
        "typedoc-plugin-markdown": "^3.17.1",
        "typeorm": "0.3.20",
        "typescript": "4.7.4",
        "uuid": "^9.0.1",
        "winston": "^3.13.0",
        "ws": "^8.16.0"
    },
    "packageManager": "yarn@4.1.1",
    "engines": {
        "node": ">=20.0.0"
    },
    "main": "dist/lib/index.js",
    "typings": "dist/types/index.d.ts",
    "files": [
        "dist",
        "docs"
    ]
}
